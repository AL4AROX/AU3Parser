echo ===== Recently created au3 files ===== > autoitactivity.txt && fsutil usn readjournal C: csv | findstr /i /c:".au3" | findstr /i /c:0x00000100 >> autoitactivity.txt && echo. >> autoitactivity.txt && echo ===== Recently deleted au3 files ===== >> autoitactivity.txt && fsutil usn readjournal C: csv | findstr /i /c:".au3" | findstr /i /c:0x80000200 >> autoitactivity.txt && echo. >> autoitactivity.txt && echo ===== Recently renamed au3 files ===== >> autoitactivity.txt && fsutil usn readjournal C: csv | findstr /i /c:".au3" | findstr /i /c:0x00001000 >> autoitactivity.txt && echo. >> autoitactivity.txt && echo ===== Previously renamed au3 files ===== >> autoitactivity.txt && fsutil usn readjournal C: csv | findstr /i /c:".au3" | findstr /i /c:0x00002000 >> autoitactivity.txt && notepad autoitactivity.txt
